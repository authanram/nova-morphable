{
    "name": "authanram/nova-morphable",
    "type": "package",
    "description": "...",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.0.1",
        "orchestra/testbench": "^6.23.0",
        "pestphp/pest": "^1.20.0",
        "pestphp/pest-plugin-mock": "^1.0.3"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\NovaMorphable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authanram\\NovaMorphable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Authanram\\NovaMorphable\\MorphableServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
